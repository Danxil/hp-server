  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.6.0"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint ./**/*.js ./*.js",
    "start": "./node_modules/.bin/babel-node index.js",
    "start-dev": "node -r dotenv/config node_modules/.bin/nodemon index.js --inspect --exec ./node_modules/.bin/babel-node index.js",
    "test-data": "node_modules/.bin/eslint ./**/*.js ./*.js && ./node_modules/babel-cli/bin/babel-node.js test-data.js",
    "test": "node_modules/.bin/eslint ./**/*.js ./*.js && node_modules/.bin/nodemon -w __tests__ -w ./ -e js,jsx --exec node_modules/.bin/jest",
    "migrate": "node -r dotenv/config node_modules/.bin/sequelize db:migrate",
    "undo-migrate": "node -r dotenv/config node_modules/.bin/sequelize db:migrate:undo:all",
    "seeds": "node -r dotenv/config node_modules/.bin/sequelize db:seed:all",
    "undo-seeds": "node -r dotenv/config node_modules/.bin/sequelize db:seed:undo:all"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
