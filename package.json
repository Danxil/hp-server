    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.3",
    "chance": "^1.0.4",
    "connect-session-sequelize": "^5.2.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "crypto-js": "^3.1.9-1",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.14.1",
    "express-session": "^1.15.6",
    "express-sslify": "^1.2.0",
    "faker": "^4.1.0",
