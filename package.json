    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^23.3.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "node-schedule": "^1.3.2",
    "auto-bind": "^1.2.1",
